#include <windows.h>
#include <ctype.h>



using namespace std;


    void mensagem_minuscula (string palavra){
    
        for (char mudar : palavra){

            mudar = tolower(mudar);

            cout << mudar;

        }
    }

    
    int main(){
        

        string palavra;

        cin >> palavra;

        mensagem_minuscula(palavra);

}

